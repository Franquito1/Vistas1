<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/back_activity2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityMultiplicar">

    <Button
        android:id="@+id/botonFib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="158dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toTopOf="@+id/resulFib"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextFib"
        tools:text="@string/Multiplicar" />

    <Button
        android:id="@+id/botonVolverFib"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="168dp"
        android:text="@string/volver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resulFib" />

    <TextView
        android:id="@+id/resulFib"
        android:layout_width="135dp"
        android:layout_height="72dp"
        android:layout_marginStart="139dp"
        android:layout_marginEnd="138dp"
        android:background="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/botonVolverFib"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonFib" />

    <EditText
        android:id="@+id/editTextFib"
        android:layout_width="285dp"
        android:layout_height="100dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/back_activity3"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/botonFib"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>